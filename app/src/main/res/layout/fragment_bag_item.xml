<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_brown"
    tools:context=".HomeFragment">

    <View
        android:id="@+id/view7"
        android:layout_width="415dp"
        android:layout_height="369dp"
        android:background="@color/DarkGray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="125dp"
        android:layout_height="139dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/frame"
        app:layout_constraintBottom_toTopOf="@+id/btnItems_bag"
        app:layout_constraintEnd_toStartOf="@+id/imageView28"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="125dp"
        android:layout_height="139dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/frame"
        app:layout_constraintBottom_toTopOf="@+id/btnItems_bag"
        app:layout_constraintEnd_toStartOf="@+id/imageView29" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="125dp"
        android:layout_height="139dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="56dp"
        android:src="@drawable/frame"
        app:layout_constraintBottom_toTopOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btnEquipment_Bag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="360dp"
        android:text="Equipment"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toStartOf="@+id/btnItems_bag"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnItems_bag"
        android:layout_width="97dp"
        android:layout_height="60dp"
        android:layout_marginBottom="360dp"
        android:text="Items"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintStart_toEndOf="@+id/btnEquipment_Bag" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="288dp"
        android:layout_marginBottom="242dp"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7"
        app:srcCompat="@drawable/potion" />

    <TextView
        android:id="@+id/txtPotionAmount"
        android:layout_width="73dp"
        android:layout_height="36dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="124dp"
        android:layout_marginEnd="303dp"
        android:layout_marginBottom="209dp"
        android:text="x XXX"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7" />

</androidx.constraintlayout.widget.ConstraintLayout>