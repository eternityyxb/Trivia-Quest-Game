<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_brown"
    tools:context=".HomeFragment">

    <View
        android:id="@+id/view7"
        android:layout_width="415dp"
        android:layout_height="369dp"
        android:background="@color/DarkGray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="125dp"
        android:layout_height="139dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/frame"
        app:layout_constraintBottom_toTopOf="@+id/btnEquipment_Bag"
        app:layout_constraintEnd_toStartOf="@+id/imageView28"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="125dp"
        android:layout_height="139dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="17dp"
        android:src="@drawable/frame"
        app:layout_constraintBottom_toTopOf="@+id/btnItems_bag"
        app:layout_constraintEnd_toStartOf="@+id/imageView29" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="125dp"
        android:layout_height="139dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="56dp"
        android:src="@drawable/frame"
        app:layout_constraintBottom_toTopOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btnEquipment_Bag"
        android:layout_width="131dp"
        android:layout_height="60dp"
        android:layout_marginBottom="360dp"
        android:text="Equipment"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toStartOf="@+id/btnItems_bag"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnItems_bag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="360dp"
        android:text="Items"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintStart_toEndOf="@+id/btnEquipment_Bag" />

    <ImageButton
        android:id="@+id/sword"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="306dp"
        android:layout_marginBottom="245dp"
        android:background="@drawable/sword"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7" />

    <ImageButton
        android:id="@+id/armor"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="296dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="245dp"
        android:background="@drawable/armor"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7" />

    <ImageButton
        android:id="@+id/helmet"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="155dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="159dp"
        android:layout_marginBottom="245dp"
        android:background="@drawable/helmet"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7" />

    <ImageButton
        android:id="@+id/btnArmor_Equiped"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="21dp"
        android:layout_marginBottom="18dp"
        android:background="@drawable/armor"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/imageView29"
        app:layout_constraintEnd_toEndOf="@+id/imageView29"
        app:layout_constraintStart_toStartOf="@+id/imageView29"
        app:layout_constraintTop_toTopOf="@+id/imageView29" />

    <ImageButton
        android:id="@+id/btnHelmet_Equiped"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="21dp"
        android:layout_marginBottom="18dp"
        android:background="@drawable/helmet"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/imageView28"
        app:layout_constraintEnd_toEndOf="@+id/imageView28"
        app:layout_constraintStart_toStartOf="@+id/imageView28"
        app:layout_constraintTop_toTopOf="@+id/imageView28" />

    <ImageButton
        android:id="@+id/btnSword_Equiped"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="21dp"
        android:layout_marginBottom="18dp"
        android:background="@drawable/sword"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/imageView26"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintStart_toStartOf="@+id/imageView26"
        app:layout_constraintTop_toTopOf="@+id/imageView26" />

    <ImageButton
        android:id="@+id/demo_necklace"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="147dp"
        android:layout_marginEnd="306dp"
        android:layout_marginBottom="122dp"
        android:background="@drawable/necklace"
        android:clickable="false"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7" />

</androidx.constraintlayout.widget.ConstraintLayout>