<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="330dp"
    android:layout_height="300dp"
    android:layout_gravity="center"
    android:background="@color/Moccasin">


    <View
        android:id="@+id/view22"
        android:layout_width="15dp"
        android:layout_height="304dp"
        android:background="@color/DarkGray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view26"
        android:layout_width="15dp"
        android:layout_height="304dp"
        android:layout_marginEnd="1dp"
        android:background="@color/DarkGray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view23"
        android:layout_width="345dp"
        android:layout_height="12dp"
        android:background="@color/DarkGray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view25"
        android:layout_width="362dp"
        android:layout_height="16dp"
        android:background="@color/DarkGray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="273dp"
        android:layout_height="70dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="12dp"
        android:fontFamily="@font/bowlby_one_sc"
        android:text="Victory"
        android:textColor="@color/Goldenrod"
        android:textSize="60sp"
        app:layout_constraintEnd_toStartOf="@+id/view26"
        app:layout_constraintStart_toEndOf="@+id/view22"
        app:layout_constraintTop_toBottomOf="@+id/view23" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="52dp"
        android:layout_height="43dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="31dp"
        app:layout_constraintStart_toEndOf="@+id/view22"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/coinicon" />

    <TextView
        android:id="@+id/win_currency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:text="+ $1000"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:text="+ 10 XP"
        app:layout_constraintStart_toEndOf="@+id/imageView14"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="41dp"
        android:layout_height="42dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="31dp"
        app:layout_constraintStart_toEndOf="@+id/win_currency"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/exp_icon" />

    <Button
        android:id="@+id/btnContinue_Victory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="94dp"
        android:text="Continue"
        app:layout_constraintEnd_toStartOf="@+id/view26"
        app:layout_constraintStart_toEndOf="@+id/view22"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <Button
        android:id="@+id/btnBackToLobby_Victory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:layout_marginEnd="73dp"
        android:layout_marginBottom="10dp"
        android:text="Back to lobby"
        app:layout_constraintBottom_toTopOf="@+id/view25"
        app:layout_constraintEnd_toStartOf="@+id/view26"
        app:layout_constraintStart_toEndOf="@+id/view22"
        app:layout_constraintTop_toBottomOf="@+id/btnContinue_Victory" />
</androidx.constraintlayout.widget.ConstraintLayout>